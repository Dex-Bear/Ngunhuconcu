function getFullMoon()
	if (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149431" then
		return "Progress 100%";
	elseif (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149052" then
		return "Progress 75%";
	else
		return false;
	end;
end;
local SendFullMoon = false;
spawn(function()
	while wait(0.2) do
		if World3 then
			if getFullMoon() and (not SendFullMoon) then
				local Config = {
					URL = "https://discord.com/api/webhooks/1368177249362120755/GVzK3UHUk461EwTGnW-19-H7Q0XW_x16rFKpnnhTV3IwxsHBQ_IKnIA_-8AocleLDjwN",
					Title = "Full Moon Finder",
					Description = "Make sure it matches the sea listed!",
					Content = {
						{
							Name = "[üåï] Full Moon",
							Value = "" .. getFullMoon() .. "",
							Inline = false
						}
					}
				};
				Webhook:Send(Config);
				SendFullMoon = true;
			end;
		end;
	end;
end);
