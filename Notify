function getFullMoon()
	if (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149431" then
		return "Progress 100%";
	elseif (game:GetService("Lighting")).Sky.MoonTextureId == "http://www.roblox.com/asset/?id=9709149052" then
		return "Progress 75%";
	else
		return false;
	end;
end;
local SendFullMoon = false;
spawn(function()
	while wait(0.2) do
		if World3 then
			if getFullMoon() and (not SendFullMoon) then
				local Config = {
					URL = "https://discord.com/api/webhooks/1354018474476703816/tCkRqrFnMPBqlKZVYc2IkiSP8ts5goWgG1GJ422tKfotyaDVAE3R2ikx8VHeDJ-gF94T",
					Title = "Full Moon Finder",
					Description = "Make sure it matches the sea listed!",
					Content = {
						{
							Name = "[√∞≈∏≈í‚Ä¢] Full Moon",
							Value = "" .. getFullMoon() .. "",
							Inline = false
						}
					}
				};
				Webhook:Send(Config);
				SendFullMoon = true;
			end;
		end;
	end;
end);
